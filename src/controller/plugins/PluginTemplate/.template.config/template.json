{
  "$schema": "http://json.schemastore.org/template",
  "author": "Team-Mpm",
  "classifications": [
    "Web",
    "Plugin"
  ],
  "identity": "MpmSmart.PluginTemplate",
  "name": "Mpm-Smart Plugin Template",
  "shortName": "plugin",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "TestPlugin",
  "preferNameDirectory": true,
  "symbols": {
    "PluginName": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "Test Plugin",
      "replaces": "Test Plugin"
    },
    "PluginRegistryName": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "test-plugin",
      "replaces": "test-plugin"
    },
    "PluginDescription": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "A test plugin",
      "replaces": "A test plugin"
    },
    "PluginAuthor": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "Gabriel Martin",
      "replaces": "Gabriel Martin"
    },
    "PluginVersion": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "1.0.0",
      "replaces": "1.0.0"
    },
    "PluginNamespace": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "MpmSmart.PluginTemplate",
      "replaces": "MpmSmart.PluginTemplate"
    }
  }
}