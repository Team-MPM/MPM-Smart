<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>4f476ada-7f3e-4841-9729-fd3155378e6a</UserSecretsId>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\ServerServiceDefaults\ServerServiceDefaults.csproj"/>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Aspire.Azure.Storage.Blobs" Version="9.0.0"/>
        <PackageReference Include="Aspire.Microsoft.EntityFrameworkCore.SqlServer" Version="9.0.0" />
        <PackageReference Include="Aspire.StackExchange.Redis" Version="9.0.0" />
        <PackageReference Include="Aspire.StackExchange.Redis.DistributedCaching" Version="9.0.0" />
        <PackageReference Include="Aspire.StackExchange.Redis.OutputCaching" Version="9.0.0" />
        <PackageReference Include="LanguageExt.Core" Version="4.4.9"/>
        <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="9.0.1" />
        <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="9.0.0">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="MudBlazor" Version="7.15.0"/>
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="UI\Pages\Account\Shared\ExternalLoginPicker.razor" />
      <_ContentIncludedByDefault Remove="UI\Pages\Account\Shared\ManageLayout.razor" />
      <_ContentIncludedByDefault Remove="UI\Pages\Account\Shared\ManageNavMenu.razor" />
      <_ContentIncludedByDefault Remove="UI\Pages\Account\Shared\RedirectToLogin.razor" />
      <_ContentIncludedByDefault Remove="UI\Pages\Account\Shared\ShowRecoveryCodes.razor" />
      <_ContentIncludedByDefault Remove="UI\Pages\Account\Shared\StatusMessage.razor" />
    </ItemGroup>

</Project>
